  "devDependencies": {
    "@types/google-protobuf": "^3.15.0",
    "@types/node": "^18.0.0",
    "grpc-tools": "^1.12.0",
    "typescript": "^4.9.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "@types/jest": "^29.0.0"
  }
}
